
<p><%= notice %></p>
<table>
  <tr>
    <th>Mytweet一覧</th>
  </tr>
<% @posts.each do |post| %>
  <tr>
   <td><%= post.content %></td>
   <td><%= link_to '詳細を確認する', post_path(post.id) %></td>
   <td><%= link_to "tweetを編集する", edit_post_path(post.id), data: { confirm: '本当に編集していいですか？' } %></td>
   <td><%= link_to 'tweetを削除する', post_path(post.id), method: :delete ,data: { confirm: '本当に削除していいですか？' } %></td>
  </tr>
<% end %>
</table>
