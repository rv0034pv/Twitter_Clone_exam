<h1>Mytweet一覧</h1>
<table>
  <tr>
    <th>内容</th>
  </tr>
<% @mytweets.each do |mytweet| %>
  <tr>
    <td><%= mytweet.content %></td>
  </tr>
  <tr>
    <td><%= link_to '詳細を確認する', mytweet_path(mytweet.id) %></td>
  </tr>
  <tr>
    <td><%= link_to "tweetを編集する", edit_mytweet_path(mytweet.id) %></td>
  </tr>
  <tr>
    <td><%= link_to 'tweetを削除する', mytweet_path(mytweet.id), method: :delete %></td>
  </tr>
<% end %>
</table>
<%= link_to '新しくtweetする', new_mytweet_path %>
